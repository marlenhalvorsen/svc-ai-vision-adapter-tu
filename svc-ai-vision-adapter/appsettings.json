{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Recognition": {
    "DefaultProvider": "google",
    "Region": null,
    "MaxResults": 5,
    "Features": [
      "LogoDetection",
      "TextDetection",
      "ObjectLocalization",
      "WebDetection"
    ]
  }
}
